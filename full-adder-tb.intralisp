[%! "full-adder.intralisp" %]
[%! "common.intralisp" %]
[% in-package :full-adder %]
[% defun tb ()
 (common:tb :name #'name :ports #'ports
:body [@^
stim_proc: process begin
[% dotimes (i 8)
 (let ((num (format nil "~3,'0b" i))) [@
    A <= '[%~ aref num 0 %]';
    B <= '[%~ aref num 1 %]';
    C_IN <= '[%~ aref num 2 %]';
    wait for 100 ns;
@])%]
  [% common:end-test :clocked nil %]
end process;                     
@]) %]
